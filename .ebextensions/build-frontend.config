container_commands:
    01 pwd:
        command: "pwd"
    02 install dependencies:
        command: "npm i"
        cwd: "./frontend"
    03 build:
        command: "npm build"
        cwd: "./frontend"
    04 move artifacts:
        command: "cp ./frontend/build/* public"
    05 cleanup:
        command: "rm -r ./frontend/build"
